<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            
            <padding android:left="10dp" android:top="10dp" android:right="10dp"
                android:bottom="10dp" />
            <corners android:radius="5dp" android:bottomRightRadius="3dp"
                android:bottomLeftRadius="3dp" android:topLeftRadius="3dp"
                android:topRightRadius="3dp" />

            <gradient android:startColor="@color/themecolorblack" android:endColor="@color/themecolorblack"
                android:angle="270" />
        </shape>
    </item>
<item android:state_pressed="true">
<shape>
   <stroke android:width="1dp" android:color="#999999" />
            <padding android:left="10dp" android:top="10dp" android:right="10dp"
                android:bottom="10dp" />
            <corners android:radius="5dp" android:bottomRightRadius="3dp"
                android:bottomLeftRadius="3dp" android:topLeftRadius="3dp"
                android:topRightRadius="3dp" />

            <gradient android:startColor="@color/themecolorblue" android:endColor="@color/themecolorblue"
                android:angle="270" />
    </shape>
    </item>
</selector>
